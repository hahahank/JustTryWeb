<script src='http://ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js'></script>

<div id="test"></div>
<script>
$.getJSON("https://spreadsheets.google.com/feeds/list/1QY-QUaQkEnbWQ0dkz0jFB7HS4HJN3Ji1NqMBOsBqseU/1/public/values?alt=json-in-script&callback=?", function(json){
var e = json.feed.entry,
l = e.length,
html = "",
entry, i, name, email, message;
for (i = 0; i < l; i++) {
entry = e[i];
name = entry.gsx$name.$t;
email = entry.gsx$email.$t;
message = entry.gsx$message.$t;
html +=  name + " " + email + " " + message + "<br/>";
}
$("#test").html(html);
});
</script>




